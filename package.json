{
  "name": "node-template-001",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.mjs",
  "type": "module",
  "scripts": {
    "clean-build": "rimraf dist tsconfig.tsbuildinfo && tsc && node rename-to-mjs.js",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "tsc && node rename-to-mjs.js",
    "start": "node dist/index.mjs",
    "prettier-unverefied": "prettier --write \"**/*.{ts,tsx,md,mdx,css,scss}\" --cache",
    "format:check-unverefied": "prettier --check \"**/*.{ts,tsx,md,mdx,css,scss}\" --cache",
    "format:write-unverefied": "prettier --write \"**/*.{ts,tsx,md,mdx,css,scss}\" --cache",
    "lint-unverefied-unverefied": "eslint . --ext .ts,.tsx",
    "lint:fix-unverefied": "eslint . --ext .ts,.tsx --fix",
    "build0-old": "tsc --b --clean && tsc --b && node rename-to-mjs.js",
    "dev-old": "tsx watch ./src/index.ts",
    "clean000-old": "tsc --b --clean",
    "start00-old": "node dist/index.js",
    "start0-old": "node --enable-source-maps dist/index.js",
    "start2-old": "node --enable-source-maps ./dist/index.js",
    "dev001-old": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build000-old": "tsc",
    "watch-old": "tsc --watch",
  },
  "dependencies": {
    "@types/nodemailer": "^6.4.14",
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.8",
    "telegraf": "^4.15.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@heiwa4126/convert-imports-to-mjs": "^1.0.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "3.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
